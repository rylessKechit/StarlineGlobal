{
  "name": "starlane-global-backend",
  "version": "1.0.0",
  "description": "Starlane Global Backend API with Express and MongoDB - Phase 1: Activities & Bookings",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --watchAll",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "seed": "node scripts/seedDatabase.js",
    "migrate": "node scripts/migrate.js",
    "seed:activities": "NODE_ENV=development node scripts/seed-activities.js",
    "seed:activities-clean": "node scripts/seed-activities.js --clean",
    "seed:services": "node scripts/seed-services.js"
  },
  "keywords": [
    "luxury",
    "services",
    "booking",
    "express",
    "mongodb",
    "activities",
    "starlane",
    "api",
    "rest"
  ],
  "author": "Starlane Global",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "muller": "^1.1.0",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starlane-global/backend.git"
  },
  "bugs": {
    "url": "https://github.com/starlane-global/backend/issues"
  },
  "homepage": "https://github.com/starlane-global/backend#readme"
}
